{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "coverageDirectory": "./coverage/",
  "collectCoverage": true,
  "setupFilesAfterEnv": [
    "./src/__tests__/jest.setup.ts"
  ],
  "modulePathIgnorePatterns": [
    "./src/__tests__/jest.setup.ts"
  ]
}