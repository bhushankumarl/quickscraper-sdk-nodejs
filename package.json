{
  "name": "quickscraper-sdk",
  "version": "1.0.0",
  "description": "A nice greeter",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": "10.16.3"
  },
  "scripts": {
    "pretest": "npm run check-version",
    "test": "jest --config jestconfig.json",
    "check-version": "node check_node_version.ts",
    "prebuild": "npm run lint",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhushankumarl/quickscraper-sdk-nodejs"
  },
  "keywords": [
    "quickscraper",
    "scraper"
  ],
  "author": "Bhushankumar L (bhushankumar.lilapara@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bhushankumarl/quickscraper-sdk-nodejs/issues"
  },
  "homepage": "https://github.com/bhushankumarl/quickscraper-sdk-nodejs#readme",
  "dependencies": {
    "got": "^11.8.1",
    "semver": "^7.3.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "codecov": "^3.8.1",
    "handlebars": "^4.7.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "lib/**/*"
  ]
}